					}
				},
				"inputs": [
					{
						"referenceName": "MoviesData",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ProccessedData",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}